<%= form_for @movie do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <br>
  <%= f.label :rating %>
  <%= f.text_field :rating %><br>
  <br>
  <%= f.label :runtime %>
  <%= f.text_field :runtime %><br>
  <br>
  <%= f.label :plot %>
  <%= f.text_field :plot %><br>
  <br>
  <%= f.label :poster %>
  <%= f.text_field :poster %><br>
  <br>
  <%= f.submit %>
<% end %>